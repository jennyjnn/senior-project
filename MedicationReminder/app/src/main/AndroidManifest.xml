<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jenny.medicationreminder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginUserActivity" />
        <activity android:name=".LoginTelActivity" />
        <activity android:name=".MenuActivity" />
        <activity
            android:name=".EditMedActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".ListMedActivity" />
        <activity android:name=".MedInfoActivity" />
        <activity
            android:name=".ProfileActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".ViewProfileActivity" />
        <activity android:name=".EditLoginActivity"></activity>
    </application>

</manifest>